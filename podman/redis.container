[Container]
ContainerName=redis
Exec=--save 60 1 --loglevel warning
HealthCmd=redis-cli ping | grep PONG
HealthInterval=30s
HealthRetries=5
HealthStartPeriod=20s
HealthTimeout=3s
Image=docker.io/library/redis:8-alpine
Label=wud.tag.include=^\\d+-alpine$$
Network=net-redis.network
Volume=/data/redis:/data

[Service]
Restart=always